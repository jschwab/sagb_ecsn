&star_job

! begin with saved model from end of core He burning
      load_saved_model = .true.
      saved_model_name = '8M_ONe_core.mod'

! save a model and profile at the end of the run
      save_model_when_terminate = .true.
      save_model_filename = 'final.mod'
      !required_termination_code_string = 'one thermal pulse cycle complete'

      write_profile_when_terminate = .true.
      filename_for_profile_when_terminate = 'final_profile.data'

! restart counters
      set_initial_age = .true.
      initial_age = 0
      set_initial_model_number = .true.
      initial_model_number = 0
      set_initial_cumulative_energy_error = .true.
      new_cumulative_energy_error = 0d0

! display on-screen plots
      pgstar_flag = .true.

/ !end of star_job namelist


&controls

! see inlist_degenerate_core

  max_model_number = 10000

/ ! end of controls namelist
