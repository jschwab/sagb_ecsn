&star_job

! network
      change_net = .true.
      new_net_name = 'sagb_NeNa_MgAl.net' ! small net to start

! opacities
      kappa_file_prefix = 'a09'
      kappa_CO_prefix = 'a09_co'
      kappa_lowT_prefix = 'AESOPUS_AGSS09'
      initial_zfracs = 6

/ !end of star_job namelist


&controls

! starting specifications
      initial_mass = 8.0
      initial_y = 0.26
      initial_z = 0.01

! convection
     mixing_length_alpha = 1.8d0
     MLT_option = 'Henyey'

     smooth_convective_bdy = .false.
     use_Ledoux_criterion = .true.

     num_cells_for_smooth_gradL_composition_term = 5
     threshold_for_smooth_gradL_composition_term = 0.02

     num_cells_for_smooth_brunt_B = 5
     threshold_for_smooth_brunt_B = 1e-4

! opacities
      use_Type2_opacities = .true.
      Zbase = 0.01d0

! options for energy conservation (see MESA V, Section 3)
      use_dedt_form_of_energy_eqn = .true.
      min_cell_energy_fraction_for_dedt_form = 0
      use_eps_mdot = .true.
      use_gold_tolerances = .true.

      num_trace_history_values = 2
      trace_history_value_name(1) = 'rel_E_err'
      trace_history_value_name(2) = 'log_rel_run_E_err'

! timestep tolerances

      varcontrol_target = 1d-4

      delta_lgRho_cntr_limit = 0.05
      delta_lgRho_cntr_hard_limit = 0.1

      delta_lgT_cntr_limit = 0.01
      delta_lgT_cntr_hard_limit = 0.02

      delta_lgTeff_limit = 0.01
      delta_lgTeff_hard_limit = 0.02

      delta_lgL_limit = 0.1
      delta_lgL_hard_limit = 0.2

! atmosphere option
      atm_option = 'table'
      atm_table = 'photosphere'
      atm_off_table_option = 'T_tau'
      atm_T_tau_relation = 'Eddington'
      atm_T_tau_opacity = 'iterated'

! mass loss
      cool_wind_RGB_scheme = 'Reimers'
      cool_wind_AGB_scheme = 'Blocker'
      RGB_to_AGB_wind_switch = 1d-4
      Reimers_scaling_factor = 0.5d0
      Blocker_scaling_factor = 0.05d0

! output
      photo_interval = 100
      profile_interval = 100
      history_interval = 10
      terminal_interval = 10
      write_header_frequency = 10


! FOR DEBUGGING

      !report_hydro_solver_progress = .true. ! set true to see info about newton iterations
      !report_ierr = .true. ! if true, produce terminal output when have some internal error
      !hydro_show_correction_info = .true.

      ! hydro debugging
      !hydro_check_everything = .true.
      !hydro_inspectB_flag = .true.
      !hydro_sizequ_flag = .true.

      !hydro_get_a_numerical_partial = 1d-4
      !hydro_test_partials_k = 751
      !hydro_numerical_jacobian = .true.
      !hydro_save_numjac_plot_data = .true.
      !hydro_dump_call_number = 124
      !hydro_dump_iter_number = 7
      !hydro_epsder_struct = 1d-6
      !hydro_epsder_chem = 1d-6
      !hydro_save_photo = .true. ! Saves a photo when hydro_call_number = hydro_dump_call_number -1

      !fill_arrays_with_NaNs = .true.
      !stop_for_NaNs = .true.

      !max_years_for_timestep = 3.67628942044319d-05

      !report_why_dt_limits = .true.
      !report_all_dt_limits = .true.
      !report_hydro_dt_info = .true.

      !show_mesh_changes = .true.
      !mesh_dump_call_number = 5189
      !okay_to_remesh = .false.

      !trace_evolve = .true.

      !trace_newton_bcyclic_solve_input = .true. ! input is "B" j k iter B(j,k)
      !trace_newton_bcyclic_solve_output = .true. ! output is "X" j k iter X(j,k)

      !trace_newton_bcyclic_matrix_input = .true.
      !trace_newton_bcyclic_matrix_output = .true.

      !trace_newton_bcyclic_steplo = 1 ! 1st model number to trace
      !trace_newton_bcyclic_stephi = 1 ! last model number to trace

      !trace_newton_bcyclic_iterlo = 2 ! 1st newton iter to trace
      !trace_newton_bcyclic_iterhi = 2 ! last newton iter to trace

      !trace_newton_bcyclic_nzlo = 1 ! 1st cell to trace
      !trace_newton_bcyclic_nzhi = 10000 ! last cell to trace; if < 0, then use nz as nzhi

      !trace_newton_bcyclic_jlo = 1 ! 1st var to trace
      !trace_newton_bcyclic_jhi = 100 ! last var to trace; if < 0, then use nvar as jhi

      !trace_k = 0


